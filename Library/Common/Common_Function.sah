			/////////////////////////// As per GDPR RO login updated -by anita ////////////
			if(_exists(_cell($obj_FirstLogin))){
											
				if($g_isGDPRCompliancepswd =="yes" || $g_isGDPRCompliancepswd =="Yes" || $g_isGDPRCompliancepswd =="YES"){					
