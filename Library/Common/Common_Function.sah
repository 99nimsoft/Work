		RemoteConnectionToWindows($compIP, $remoteUserNameVal, $remotePwdVal, $Command,"Y");
		$Command = "SC delete SFR";
		RemoteConnectionToWindows($compIP, $remoteUserNameVal, $remotePwdVal, $Command,"Y");
		
		var $Command = "RMDIR /Q /S \"" + $UserInstallDirforRemote + "\"";
		RemoteConnectionToWindows($compIP, $remoteUserNameVal, $remotePwdVal, $Command,"Y");
	} else {	
		
		
		
		$Command = "SC stop WindowsOSAgent";
