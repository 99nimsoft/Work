function Trigger_Ral_Start_SO($groupName,$workflowType,WorkflowNameTwo){
	createWorkFlow($groupName,$WorkflowNameTwo);
	navigateDrillListing($groupName);
	$versionNo = _getText(_span(0, _rightOf(_link($WorkflowNameTwo))));
	
	if("Version: 0" == $versionNo){		
		
		NavigateToDrillWorkflowEditor($groupName,$WorkflowNameTwo);
		AddRalIntoLayout($ralString,$ralCategory,$ralNameIE);
	} else {
		NavigateToDrillWorkflowEditor($groupName,$WorkflowNameTwo);
	}
	editTriggerTEAction($groupName,$workflowType);
	publishWorkflow();
	executeWorkflow($groupName,$WorkflowNameTwo);
	Drills_Workflow_Execution_Status($WorkflowNameTwo,$ralNameIE);
}

