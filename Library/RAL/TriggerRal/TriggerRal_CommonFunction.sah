function Trigger_Ral_InitiateStop_NcRnc($groupName,$workflowType,$WorkflowName){
	if($workflowType ==$InitiateReverseNormalCopy || $workflowType == $StopReverseNormalCopy||
		$workflowType ==$InitiateFailover || $workflowType == $InitiateFallback	){
		navigateDrillListing($groupName);
		NavigateToWorkflowEditor($WorkflowName);
	}
	
	editTriggerBCO($groupName,$workflowType);
	publishWorkflow();
	executeWorkflow($groupName,$WorkflowName);
}





function editTriggerTEAction($groupName,$workflowType){
	_click(_paragraph("text_ellipsis"));
	_click(_button("infoBtn"));
	_click(_link($ActionProperties));
	_setSelected(_select("groupName"), $groupName);
	_wait(5000);
	_setSelected(_select("testName"), $workflowType);
	_click(_select("testName"));
	_click(_div("myModal"));
}


function AddRalIntoLayout($ralString,$ralCategory,$ralName){
	_click(_span("glyphIon"));
	_click(_link("select2-choice"));
	_setValue(_textbox(10), $ralString);
	_click(_div($ralCategory));
	_click(_span("glyphicon icon-blue-plus plusIconRals", _near(_span($ralName))));
	_click(_span("glyphIon"));
	_dragDrop(_image("notifications_green.png"),_image("notifications_yellow.png"));
}

function editTriggerBCO($groupName,$workflowType){
	_click(_paragraph("text_ellipsis"));
	_click(_button("infoBtn"));
	_click(_link($ActionProperties));
	_setSelected(_select("groupName"), $groupName);
    _setSelected(_select("selectTheTrigger"),$workflowType);
  	_click(_select("selectTheTrigger"));
	_click(_div("myModal"));
}


