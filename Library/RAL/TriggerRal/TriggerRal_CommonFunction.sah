function  Trigger_Ral_InitiateOrStop_NcRnc($groupName,$workflowType,$WrkflwNm){
	createWorkFlow($groupName,$WrkflwNm);
	navigateDrillListing($groupName);
		$versionNo = _getText(_span(0, _rightOf(_link($WrkflwNm))));
		
		if("Version: 0" == $versionNo){		
			
			NavigateToDrillWorkflowEditor($groupName,$WrkflwNm);
			AddRalIntoLayout($ralString,$ralCategory,$ralName);
		} else {
			NavigateToDrillWorkflowEditor($groupName,$WrkflwNm);
		}
	
	Trigger_Ral_InitiateStop_NcRnc($groupName,$workflowType,$WrkflwNm);
}

