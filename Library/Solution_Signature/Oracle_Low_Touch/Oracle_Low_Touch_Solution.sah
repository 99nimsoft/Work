}


function Verfiy_Protection_Schemes_DR(){
	Oracle_Low_Verfiy_Protection_Schemes($CompAmazonInstanceIP);
}

function Oracle_Low_Verfiy_Protection_Schemes($Comp){
	_click(_link("Admin"));
	navigateToDiscovery();
	_click(_link("Groups"));
	_click(_link("Subsystems"));
	_click(_link("Protection Schemes"));
	 var $Protection_Schemes = "Oracle_DG_"+$Comp;
	 _assertExists(_link($Protection_Schemes));
	   _assertExists(_cell("ACTIVE", _near(_cell($Protection_Schemes))));
}



