
public class A{
	
	public static void main(String[] args) {
		
		mahList c = new mahList();
	}
}

class mahList{
	
	Object[] arr;
	int size;
	
	mahList(){
		arr = new Object[10];
	}
	
	void add(Object obj){
		validateCapacity(size+1);
		arr[size++]=obj;
	}
	
	void validateCapacity(int z){
		if(z>arr.length){
			int tmp = ((arr.length*3)/2)+1;
			arr=new Object[tmp];
		}
	}
}