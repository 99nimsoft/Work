
public class A{
	
	public static void main(String[] args) {
		
		mahList c = new mahList();
		c.add("qqq");
		c.add("www");
		c.add("eee");
		
		for(int i=0;i<c.size;i++){
			System.out.println(c.get(i));
		}
	}
}

class mahList{
	
	Object[] arr;
	//int size=3;
	
	mahList(){
		arr = new Object[10];
	}
	
	void add(Object obj){
		validateCapacity(size+1);
		arr[size++]=obj;
	}
	
	void validateCapacity(int z){
		if(z>arr.length){
			int tmp = ((arr.length*3)/2)+1;
			arr=new Object[tmp];
		}
	}
	
	Object get(int i){
		return arr[i];
	}
}