
public class A{

	public static void main(String[] args) {
		int[][] matrix = new int[4][4];
		for(int i=0; i < 3; i++) {
			for(int j=0; j < 3; j++) {
				matrix[i][j]=j;
			}
		}
		
		
		for(int i=0; i < 3; i++) {
			for(int j=0; j < 3; j++) {
				System.out.print("\t" + matrix[i][j]);
			}
			System.out.println();
		}
	}
}