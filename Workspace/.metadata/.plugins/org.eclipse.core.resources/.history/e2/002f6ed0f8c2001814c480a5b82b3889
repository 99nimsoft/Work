
public class A{

	
	public static void main(String[] args) {
		Bb b = new Bb();
		String tmp1 = b.a="neelu";
		String tmp2 = b.b="cheelu";
		System.out.println(tmp1+" : "+tmp2);
		
		Bb b1 = new Bb();
		System.out.println(b1.a+" : "+b1.b);
	}
	
}

class Bb{
	String a = "gaurav";
	static String b = "saurabh";
}